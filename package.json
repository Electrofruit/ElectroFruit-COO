{
  "name": "ewf-coo",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-matcher": "rm -rf build && truffle migrate && node_modules/typescript/bin/tsc && mocha build/ts/test",
    "build-ts": "rm -rf build/ts && node_modules/typescript/bin/tsc",
    "start-matcher": "rm -rf build/ts && node build/ts/matcher-main.js",
    "compile": "rm -rf build && truffle compile",
    "migrate": "rm -rf build && truffle migrate",
    "deploy": "rm -rf build && truffle deploy",
    "test": "rm -rf build && truffle compile && truffle test",
    "startSim": "truffle exec contract-scripts/simulation.js",
    "coverage": "./node_modules/.bin/solidity-coverage",
    "wincompile": "rmdir build /s && truffle compile",
    "winmigrate": "rmdir build /s && truffle migrate",
    "windeploy": "rmdir build /s && truffle deploy",
    "wintest": "rmdir build /s && truffle compile && truffle test",
    "remixd": "open https://remix.ethereum.org/#version=soljson-v0.4.17+commit.bdeb9e52.js; remixd -S $(pwd)/contracts",
    "start-ganache": "ganache-cli -m 'outer moment twin salmon shuffle amused twice evil rapid hotel burst stand' -g 0 -l 80000000 -e 1000000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slockit/ewf-coo.git"
  },
  "Dependencies": {
    "remixd": "^0.1.2"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slockit/ewf-coo/issues"
  },
  "homepage": "https://github.com/slockit/ewf-coo#readme",
  "dependencies": {
    "remixd": "^0.1.2",
    "solidity-coverage": "^0.4.9",
    "typescript": "^2.7.1",
    "web3": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.5",
    "chai": "^4.1.2",
    "mocha": "^5.0.1"
  }
}